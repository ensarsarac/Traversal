
@model List<AppUser>

@{
    ViewData["Title"] = "GetUsers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Kullanıcılar</h1>
<hr />

<table class="table table-striped">

    <tr>
        <th>Kullanıcı Adı</th>
        <th>Rol Ata</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td><a href="/Admin/Role/UserGetRole/@item.Id" class="btn btn-warning">Rol Ata</a></td>
        </tr>
    }


</table>

